<!--使用DTD验证模式-->
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE note[
        <!ELEMENT note (to,from,heading,body)>
        <!ELEMENT to (#PCDATA)>
        <!ELEMENT from (#PCDATA)>
        <!ELEMENT heading (#PCDATA)>
        <!ELEMENT body (#PCDATA)>
]>
<note>
    <to>jerry</to>
    <from>tom</from>
    <heading>eating</heading>
    <body>go eat</body>
</note>